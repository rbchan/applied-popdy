\begin{frame}
  \frametitle{How does this population grow?}
  \Large
  \begin{center}
    \begin{tabular}{cl}
      \hline
      Age class & Equation \\
      \hline
      1 & $n_{1,t+1} = n_{1,t} \times f_1 + n_{2,t} \times f_2 + n_{3,t} \times f_3$ \\ \pause
      2 & $n_{2,t+1} = n_{1,t} \times s_{1}$ \\ \pause
      3 & $n_{3,t+1} = n_{2,t} \times s_{2}$ \\
      \hline
    \end{tabular}
  \end{center}
\end{frame}





